---
import CurrentYearAstro from "./CurrentYearAstro.astro";

export interface Props {
  category: string;
  initialUrl: string;
}

const { category, initialUrl } = Astro.props as Props;
---

<a href={initialUrl} class="btn btn-primary btn-lg mb-3 w-100">
  Rozpocznij naukę testów na prawo jazdy <CurrentYearAstro />
  <strong>kategorii: {category.toUpperCase()}</strong></a
>
